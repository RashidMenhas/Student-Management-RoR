<p><%= notice %></p>
<h1>Listing Students.</h1>
<td><%= link_to 'Add New Student ', new_student_path%></td>
<br/>
<br/>
<table>
  <thead>
    <tr>
      <th>Fist Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th colspan='4'>Action</th>
    </tr>
  </thead>
  <tbody>
    <% @students.each do |student|%>
      <tr>
        <td><%= student.first_name%></td>
        <td><%= student.last_name%></td>
        <td><%= student.email%></td>
        <td><%= link_to 'View', student%></td>
        <td><%= link_to 'Edit', edit_student_path(student)%></td>
        <td><%= link_to 'Delete', student, method: :delete, data: {confirm: 'Are you sure to delete.?'}%></td>
      </tr>
    <% end %>
  </tbody>
</table>
